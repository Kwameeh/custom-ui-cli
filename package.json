{
  "name": "@kwameeh_1/custom-ui-cli",
  "version": "1.0.0",
  "description": "A CLI tool for installing and managing custom UI components",
  "main": "dist/index.js",
  "bin": {
    "custom-ui": "dist/index.js",
    "cui": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "validate-registry": "ts-node src/registry/validate.ts",
    "prepublishOnly": "npm run build && npm run test && npm run validate-registry",
    "prepack": "npm run build"
  },
  "keywords": [
    "ui",
    "components",
    "react",
    "typescript",
    "tailwind",
    "cli"
  ],
  "author": "Custom UI Team",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.5.0",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "inquirer": "^8.2.6",
    "lucide-react": "^0.540.0",
    "ora": "^5.4.1",
    "semver": "^7.5.4",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^30.0.5",
    "jsdom": "^26.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kwameeh/custom-ui-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Kwameeh/custom-ui-cli/issues"
  },
  "homepage": "https://github.com/Kwameeh/custom-ui-cli#readme"
}